{
	"compilerOptions": {
		"sourceMap": true,
		"target": "ES5",
		"removeComments": false,
		"strict": true,
		"alwaysStrict": true
	},
	"include": ["src/**/*"],
	"exclude": ["node_modules"],
	"typeRoots": ["node_modules/@types"],
	"typedocOptions": {
		"inputFiles": ["./src"],
		"mode": "file",
		"out": "docs"
	}
}
